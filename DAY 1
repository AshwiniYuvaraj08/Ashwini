import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    String name;
		Scanner kbd=new Scanner(System.in);
		name=kbd.nextLine();
		System.out.println(name);
	    
	System.out.println(name.charAt(2));
	}
}

OUTPUT:
ASHWINI
H
