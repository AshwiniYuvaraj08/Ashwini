import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int Ascii= sc.nextInt();
           char character= (char)Ascii; 
           System.out.println(Ascii+":"+character);
}
}
OUTPUT:
78
78:N
