import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
    Scanner kbd=new Scanner(System.in);
    int n=kbd.nextInt();
    if(n%2==0){
        System.out.println("Even");
        if(n>0){
            System.out.println("Pos");
        }
        else{
            System.out.println("Neg");
        }
        
    }
    else{ 
        System.out.println("Odd"); 
        if(n>0){
            System.out.println("Pos");
        }
        else{
            System.out.println("Neg");
        }
    }
    }
}
OUTPUT:
8
  POS
  EVEN
-4
  EVEN
  NEG
