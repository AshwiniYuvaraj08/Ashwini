 import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
    Scanner kbd=new Scanner(System.in);
    int n=kbd.nextInt();
    System.out.print((n%2==0)?("even"):("odd"));
   
    }
}
OUTPUT:
8
  EVEN
7
  ODD
